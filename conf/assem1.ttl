@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .

@prefix ja:         <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix ex:         <http://example/> .

## ----------------
## In-memory, loaded with a file 

ex:a1 a ja:MemoryModel ;
    ja:content [ja:externalContent <file:Data/test_data.n3>]
    .

## ----------------
## Database, named model.

ex:a2 rdf:type ja:RDBModel ;
    ja:connection
    [
        ja:dbType "MySQL" ;
        ja:dbURL          <jdbc:mysql://localhost/data> ;
        ja:dbUser         "user" ;
        ja:dbPassword     "password" ;
        ja:dbClass        "com.mysql.jdbc.Driver" ; # Ignored
    ] ;
    ja:modelName "books" 
    . 

## ----------------
## Database, default model.

ex:a3 rdf:type ja:RDBModel ;
    ja:connection
    [
        ja:dbType "MySQL" ;
        ja:dbURL          <jdbc:mysql://localhost/data> ;
        ja:dbUser         "user" ;
        ja:dbPassword     "password" ;
        ja:dbClass        "com.mysql.jdbc.Driver" ;
    ] ;
    ja:modelName "DEFAULT" # BROKEN
    . 

## ----------------
## RDFS reasoner- one input file.

ex:a4
    ja:baseModel 
        [ a ja:MemoryModel ;
          ja:content [ja:externalContent <file:Data/test_data_rdfs.ttl>] ;
        ] ;
    ja:reasoner
         [ ja:reasonerURL <http://jena.hpl.hp.com/2003/RDFSExptRuleReasoner> ] 
    . 
## ----------------
## RDFS reasoner- separate abox and tbox  input file.
ex:a5
    rdfs:label "foo" ;
    ja:baseModel 
        [ a ja:MemoryModel ;
          ja:content [ja:externalContent <file:Data/test_abox.ttl>] ;
    rdfs:label "foo" ;
          ja:content [ja:externalContent <file:Data/test_tbox.ttl>] ;
        ] ;
    ja:reasoner
         [ ja:reasonerURL <http://jena.hpl.hp.com/2003/RDFSExptRuleReasoner> ] 
    . 

